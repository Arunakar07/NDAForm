{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "nda-popup-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ndaPopup/NdaPopupWebPart.js",
          "manifest": "./src/webparts/ndaPopup/NdaPopupWebPart.manifest.json"
        }
      ]
    },
    "nda-extension-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/ndaExtension/NdaExtensionApplicationCustomizer.js",
          "manifest": "./src/extensions/ndaExtension/NdaExtensionApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "NdaPopupWebPartStrings": "lib/webparts/ndaPopup/loc/{locale}.js",
    "NdaExtensionApplicationCustomizerStrings": "lib/extensions/ndaExtension/loc/{locale}.js"
  }
}
